{
    "compilerOptions": {
      "outDir": "./dist/",
      "module": "commonjs",
      "noImplicitAny": true,
      "allowJs": true,
      "esModuleInterop": true,
      "target": "es6",
      "strict": true,
      "sourceMap":  true
    },
    "include": ["./src/**/**/*"],
    "exclude": ["node_modules"],
    "typeAcquisition": {
      "enable": true
    }
  }